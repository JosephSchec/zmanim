{
  "expo": {
    "name": "zmanim",
    "slug": "zmanim",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/playstore.png",
    "splash": {
      "image": "./assets/playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#1BD5FC"
    },
    "updates": {
